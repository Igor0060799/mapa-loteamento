async function carregarPlanilha() {
    try {
        console.log("🔄 Buscando dados da API...");
        const response = await fetch(API_URL);
        const text = await response.text();  // Lê como texto primeiro
        console.log("📜 Resposta da API:", text);

        let data = JSON.parse(text);  // Tenta converter para JSON
        console.log("✅ Dados JSON recebidos:", data);

        if (!Array.isArray(data) || data.length === 0) {
            throw new Error("Nenhum dado válido recebido.");
        }

        // Continua com a lógica de exibição dos dados...
    } catch (error) {
        console.error("❌ Erro ao carregar dados:", error);
        alert("Erro ao carregar os dados da planilha.");
    }
}
